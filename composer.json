{
    "name": "altvel/altvel",
    "description": "High Speed PHP Web Application Framework For PHP Engineers. Not Recommended For API.",
    "type": "project",
    "version": "v0.1.0",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3.0",
        "gladcodes/ravephp": "1.0.0",
        "jenssegers/blade": "v1.3.0",
        "php-di/php-di": "v6.0.11",
        "sevens/model-trait": "v1.2.1",
        "sevens/uploader-trait": "v0.9.0",
        "sevens/vars": "v1.0.1",
        "symfony/http-foundation": "5.0.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Providers\\": "app/Providers/",
            "App\\Helpers\\": "app/Helpers/",
            "App\\Controllers\\": "app/Controllers/"
        },
        "files": [
                "app/Helpers/Helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "@php Engineer app::start"
        ]
    },
    "keywords": [
                "altvel",
                "laravel", "framework",
                "engineer"
    ]
}
